command /fix2 [<string>]:
    trigger:
        loop {cache::*}:
            if {eco::%loop-index%::join} is true:
                loop {eco::%loop-index%::achv-complete::*}:
                    set (yaml value "%loop-index-1%.%loop-index-2%" from "completed") to 1
        save yaml "completed"
        broadcast "Gotowe"

